<%- if @enable_log_file -%>
LogFile="YES"
<%- else -%>
LogFile="NO"
<%- end -%>
LogFileName="<%= @log_file %>"
LogFileSize="<%= @log_file_size %>"
<%- if @enable_trace_file -%>
TraceFile="YES"
<%- else -%>
TraceFile="NO"
<%- end -%>
TraceFileName="<%= @trace_file %>"
TraceFileSize="<%= @trace_file_size %>"
TraceConfigFile="<%= @trace_config_file %>"
